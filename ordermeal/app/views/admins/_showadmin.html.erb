<%=notice%>
<%=link_to "首页","/"%>
<h1>管理员列表</h1>

<table border="1">
  <tr>
    <th>id</th>
    <th>用户名</th>
    <th>密码</th>
    <th>性别</th>
    <th>真实姓名</th>
    <th>电话</th>
    <th>电子邮箱</th>
    
  </tr>

<% @admins.each do |admin| %>
  <tr>
    <td><%= admin.id %></td>
    <td><%= admin.name %></td>
    <td><%= admin.password %></td>
    <td><%= admin.sex %></td>
    <td><%= admin.username %></td> 
    <td><%= admin.tel %></td>
    <td><%= admin.email %></td>
    <td><%= link_to '显示', admin %></td>
    <td><%= link_to '编辑', edit_admin_path(admin) %></td>
    <td><%= link_to '删除', admin, method: :delete, data: { confirm: '确定删除吗?' } %></td>
  </tr>
<% end %>
</table>

<br />